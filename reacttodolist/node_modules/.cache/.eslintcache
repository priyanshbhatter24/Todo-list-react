ncompleted\":\n        setFilteredTodos(todos.filter(todo => todo.completed === false));\n        break;\n      default:\n        setFilteredTodos(todos);\n        break;\n    }\n  };\n\n//save to local storage\n  const saveLocalTodos = () => {\n    localStorage.setItem(\"todos\", JSON.stringify(todos));  \n  };\n\n  const getLocalTodos = () => {\n    if(localStorage.getItem(\"todos\") === null) {\n      localStorage.setItem(\"todos\", JSON.stringify([]));\n    } else {\n      let todoLocal = JSON.parse(localStorage.getItem('todos'));\n      setTodos(todoLocal);\n    }\n  };\n\n  return (\n    <div className=\"App\">\n      <div className=\"glassTop\">\n        <header className=\"project-name\">Todo List</header>\n        <Heading className=\"filter-head\" setStatus={setStatus}/>\n      </div>\n    \n      <Form todos={todos} inputText={inputText} setTodos={setTodos} setInputText={setInputText} setStatus={setStatus} />\n      <TodoList setTodos={setTodos} todos={todos} filteredTodos={filteredTodos} />\n    </div>\n  );\n}\n\nexport default App;","/Users/priyansh/Desktop/learnjs/Todo-list-react/reacttodolist/src/reportWebVitals.js",[],[],"/Users/priyansh/Desktop/learnjs/Todo-list-react/reacttodolist/src/components/Form.js",["78"],[],"import React from 'react';\n\nconst Form = ({ setInputText, todos, setTodos, inputText, setStatus }) => {\n    const inputTextHandler = (e) => {\n        setInputText(e.target.value);\n    };\n    const submitTodoHandler = (e) => {\n        e.preventDefault();\n        setTodos([\n            ...todos, {text: inputText, completed: false, id: Math.random()*10000}\n        ]);\n        setInputText(\"\");\n\n    };\n    const statusHandler = (e) => {\n        setStatus(e.target.value);\n    }\n    return (\n        <form>\n            <input value={inputText} onChange={inputTextHandler} type=\"text\" className=\"todo-input\" />\n            <button onClick={submitTodoHandler} className=\"todo-button\" disabled={inputText? false : true} type=\"submit\">\n                <i className=\"fas fa-plus-square\"></i>\n            </button>\n        </form>\n    );\n};\n\nexport default Form;","/Users/priyansh/Desktop/learnjs/Todo-list-react/reacttodolist/src/components/TodoList.js",[],[],"/Users/priyansh/Desktop/learnjs/Todo-list-react/reacttodolist/src/components/Heading.js",[],[],"/Users/priyansh/Desktop/learnjs/Todo-list-react/reacttodolist/src/components/Todo.js",[],[],{"ruleId":"79","severity":1,"message":"80","line":21,"column":6,"nodeType":"81","endLine":21,"endColumn":21,"suggestions":"82"},{"ruleId":"83","severity":1,"message":"84","line":15,"column":11,"nodeType":"85","messageId":"86","endLine":15,"endColumn":24},{"ruleId":"79","severity":1,"message":"80","line":21,"column":6,"nodeType":"81","endLine":21,"endColumn":21,"suggestions":"87"},{"ruleId":"83","severity":1,"message":"84","line":15,"column":11,"nodeType":"85","messageId":"86","endLine":15,"endColumn":24},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'filterHandler' and 'saveLocalTodos'. Either include them or remove the dependency array.","ArrayExpression",["88"],"no-unused-vars","'statusHandler' is assigned a value but never used.","Identifier","unusedVar",["89"],{"desc":"90","fix":"91"},{"desc":"90","fix":"92"},"Update the dependencies array to be: [filterHandler, saveLocalTodos, status, todos]",{"range":"93","text":"94"},{"range":"95","text":"94"},[536,551],"[filterHandler, saveLocalTodos, status, todos]",[536,551]]